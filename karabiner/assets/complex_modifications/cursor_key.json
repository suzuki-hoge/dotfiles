{
  "title": "5. cursor key",
  "rules": [
    {
      "description": "down",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": { "mandatory": [ "right_shift" ] }
          },
          "to": [
            { "key_code": "down_arrow" }
          ]
        }
      ]
    },
    {
      "description": "left",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": { "mandatory": [ "right_shift" ] }
          },
          "to": [
            { "key_code": "left_arrow" }
          ]
        }
      ]
    },
    {
      "description": "up",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": { "mandatory": [ "right_shift" ] }
          },
          "to": [
            { "key_code": "up_arrow" }
          ]
        }
      ]
    },
    {
      "description": "right",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "g",
            "modifiers": { "mandatory": [ "right_shift" ] }
          },
          "to": [
            { "key_code": "right_arrow" }
          ]
        }
      ]
    }

  ]
}

