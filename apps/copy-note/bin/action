group=$1
key=$2

val=`jq -r ".[] | select(.group == \"$1\") | .items[] | select(.key == \"$2\") | .val" ~/.copy-note.json`
action=`jq -r ".[] | select(.group == \"$1\") | .action" ~/.copy-note.json`

case "$action" in
  "browser"    ) open -a Google\ Chrome $val;;
  "clip board" ) echo "$val" | pbcopy;;
  *            ) echo "$val";;
esac
