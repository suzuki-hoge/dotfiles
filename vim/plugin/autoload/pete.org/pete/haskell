" vim:set ft=vim:

python <<EOM

class Haskell(Base):

	def __init__(self):
		Base.__init__(self)

	def getHeadMark(self):
		return '--'

	def getTailMark(self):
		return ''

	def run(self):
		vim.command('!/Applications/ghc-7.8.4.app/Contents/bin/ghc -e main %')

	def getDebugLines(self, var):
		pass

EOM
