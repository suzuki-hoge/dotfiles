" vim:set ft=vim:

python <<EOM

class File(Entry):

	def __init__(self, root, fullpath):
		Entry.__init__(self, root, fullpath)

	def getOutputs(self, result):
		return result.append(self.createOutput())

	def pointSwitch(self, num):
		if self.num == num:
			if self._isPointed():
				self.pointed = False
			else:
				self.pointed = True

EOM
