" vim:set ft=vim:

python <<EOM

class Fold():

	def __init__(self):
		self.value = False

	def on(self):
		self.value = True

	def off(self):
		self.value = False

	def isOn(self):
		return self.value

	def isOff(self):
		return not self.isOn()

	def getPartOfOutputAtDir(self):
		return '>' if self.isOn() else 'v'

	def getPartOfOutputAtFile(self):
		return ' '

EOM
