python <<EOM

class File(Entry):

	def __init__(self, root, path):
		Entry.__init__(self, root, path)
		self.type = Type(Type.FILE)

	def createOutput(self):
		return '%2d : %s %s%s%s%s' % (self.num.value, self.fold.getPartOfOutputAtFile(), self.point.getPartOfOutput(), self.depth.getPartOfOutput(), self.path.getPartOfOutput(), self.type.getPartOfOutputAtFile())

	def getOutputs(self, result):
		return result.append(self.createOutput())

	def pointOn(self, target):
		if self.num.isTarget(target):
			self.point.on()

	def pointOff(self, target):
		if self.num.isTarget(target):
			self.point.off()

	def foldOn(self, target):
		pass

	def foldOff(self, target):
		pass

	def recFoldOn(self, target):
		pass

	def _recFoldOn(self):
		pass

	def numbering(self):
		self.num = Num()

EOM
