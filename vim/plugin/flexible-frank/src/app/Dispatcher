" vim:set ft=vim:

function! ActionDispatch(action)
python <<EOM
action = vim.eval('a:action')

if action == 'dump':
	dump()

elif action == 'Dump':
	Dump()

elif action == 'Reload':
	Reload()

elif action == 'PointOn':
	PointOn()

elif action == 'PointOff':
	PointOff()

elif action == 'MoveUp':
	MoveUp()

elif action == 'MovePrev':
	MovePrev()

elif action == 'ChangeDir':
	ChangeDir()

elif action == 'FoldOn':
	FoldOn()

elif action == 'FoldOff':
	FoldOff()

elif action == 'RecFoldOn':
	RecFoldOn()

elif action == 'RecFoldOff':
	RecFoldOff()

elif action == 'AllFoldOn':
	AllFoldOn()

elif action == 'AllFoldOff':
	AllFoldOff()

elif action == 'CloseAll':
	Tab.closeAll()

elif action == 'edit':
	edit()

elif action == 'Edit':
	Edit()

EOM
endfunction
