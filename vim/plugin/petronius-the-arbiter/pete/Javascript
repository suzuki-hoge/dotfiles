" vim:set ft=vim:

python <<EOM

class Javascript(Base):

	def __init__(self):
		Base.__init__(self)

	def getHeadMark(self):
		return '//'

	def getTailMark(self):
		return ''

	def run(self):
		self.browserReload()

	def getDebugLines(self, var):
		return [
			'',
			'console.log(%s)' % var,
			''
		]

EOM
