" vim:set ft=vim:

python <<EOM

class Php(Base):

	def __init__(self):
		Base.__init__(self)

	def getHeadMark(self):
		return '//'

	def getTailMark(self):
		return ''

	def run(self):
		self.browserReload()

	def getDebugLines(self, var):
		return [
			"echo '<pre>';",
			'var_dump($%s);' % var,
			'exit;'
		]

EOM
